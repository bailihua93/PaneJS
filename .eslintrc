{
  "extends": "airbnb/base",
  "env": {
    "browser": true,
    "node": true
  },
  "ecmaFeatures": {
    "modules": true
  },
  "rules": {
    "func-names": 0,
    "indent": [
      2,
      4
    ],
    "no-var": 0,
    "prefer-const": 0,
    "vars-on-top": 0,
    "comma-dangle": 0,
    "padded-blocks": 0,
    "space-before-function-paren": 0,
  }
}
