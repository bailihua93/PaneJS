{
  "name": "pane",
  "version": "0.0.1",
  "description": "JavaScript diagramming library",
  "main": "index.js",
  "homepage": "https://github.com/PaneJS/PaneJS",
  "keywords": [
    "diagramming",
    "BPMN",
    "UML"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/PaneJS/PaneJS.git"
  },
  "author": {
    "description": "team of PaneJS",
    "url": "https://github.com/orgs/PaneJS/teams"
  },
  "bugs": {
    "url": "https://github.com/PaneJS/PaneJS/issues"
  },
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^5.8.29",
    "babel-loader": "^5.3.2",
    "eslint": "^1.7.3",
    "precommit-hook": "~3.0.0",
    "webpack": "^1.12.2"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "eslint src index.js --ext '.js'",
    "compile": "npm run clean && webpack && webpack --config webpack.config.min.js",
    "release": "npm run clean && webpack --config webpack.config.production.js && webpack --config webpack.config.min.js && zip dist/${npm_package_name}-${npm_package_version}.zip -j dist dist/*",
    "test": "browserify spec/index.spec.js > spec/index.js ",
    "validate": "npm ls"
  },
  "pre-commit": [
    "lint",
    "test"
  ]
}
